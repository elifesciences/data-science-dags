[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    too-few-public-methods,
    too-many-arguments,
    too-many-locals,
    too-many-branches,
    too-many-instance-attributes,
    too-many-public-methods,
    no-else-return,
    duplicate-code,
    fixme,
    unspecified-encoding,
    consider-using-f-string

[TYPECHECK]
ignored-classes=
    lxml.builder.ElementMaker,
    lxml.builder.E
extension-pkg-whitelist=
    lxml

[IMPORTS]
known-third-party=
    airflow
